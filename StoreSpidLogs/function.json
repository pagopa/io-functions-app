{
  "bindings": [
    {
      "queueName": "spidmsgitems",
      "connection": "LogStorageConnection",
      "name": "spidMsgItem",
      "type": "queueTrigger",
      "direction": "in"
    }
    ,
    {
      "type": "blob",
      "name": "spidRequest",
      "path": "spidassertions/{createdAtDay}-{spidRequestId}-{payloadType}.json",
      "connection": "LogStorageConnection",
