    },
    {
      "type": "blob",
      "name": "spidResponse",
      "path": "spidassertions/{createdAtDay}-{spidRequestId}-{payloadType}-{fiscalCode}.json",
      "connection": "LogStorageConnection",
      "direction": "out"
    }
  ],
  "disabled": false,
  "scriptFile": "../dist/StoreSpidLogs/index.js"
}
