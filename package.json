    "@types/jest": "^24.0.15",
    "@types/lolex": "^3.1.1",
    "@types/nodemailer": "^4.6.8",
    "@types/superagent": "^4.1.3",
    "danger": "^4.0.2",
    "danger-plugin-digitalcitizenship": "^0.3.1",
    "fast-check": "^1.16.0",
    "italia-tslint-rules": "^1.1.3",
    "jest": "^24.8.0",
    "jest-mock-express": "^0.1.1",
    "lolex": "^5.1.1",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
